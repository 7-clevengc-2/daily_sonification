# Environment variables for the soundscape server
# Copy this file to .env and update the values as needed

# JWT Secret for token signing (REQUIRED for production)
# Generate a secure random string for production
JWT_SECRET=your-secure-jwt-secret-key-here

# Admin key for data export (REQUIRED for production)
# Generate a secure random string for production
ADMIN_KEY=your-secure-admin-key-here

# Port number (optional - Render will set this automatically)
PORT=3001 